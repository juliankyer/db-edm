{
    "extends": "airbnb-base",
    "plugins": [
        "import"
    ],
    "globals": {
      "it": false,
      "describe": false,
      "should": false,
      "before": false,
      "beforeEach": false
    },
    "rules": {
      "arrow-body-style": "off",
      "consistent-return": "off",
      "no-console": "off"
    }
}